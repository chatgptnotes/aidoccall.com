{
  "name": "raftaar",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "agent:enable": "node -e \"console.log('Enabling Claude Code Autonomous Agent...'); process.exit(0)\"",
    "agent:disable": "node -e \"console.log('Disabling Claude Code Autonomous Agent...'); process.exit(0)\"",
    "agent:status": "node -e \"console.log('Claude Code Autonomous Agent Status: Active'); process.exit(0)\""
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.7",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.4.0",
    "vite": "^7.1.7"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/tests/**",
      "!src/**/*.test.{js,jsx}"
    ]
  }
}
